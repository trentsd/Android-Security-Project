<?xml version="1.0" encoding="utf-8"?>
<LinearLayout style="@style/AppAlertDialog"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <com.discord.app.AppTextView android:id="@id/server_settings_confirm_disable_integration_header" android:text="@string/disable_integration_title" style="@style/AppAlertDialogHeader" />
    <LinearLayout style="@style/AppAlertDialogContent">
        <com.discord.app.AppTextView android:id="@id/server_settings_confirm_disable_integration_body" style="@style/AppAlertDialogText" />
    </LinearLayout>
    <LinearLayout style="@style/AppAlertDialogFooter">
        <TextView android:id="@id/server_settings_confirm_disable_integration_cancel" android:text="@string/cancel" style="@style/AppAlertDialogButton" />
        <TextView android:id="@id/server_settings_confirm_disable_integration_confirm" android:text="@string/disable" style="@style/AppAlertDialogButton.Red" />
    </LinearLayout>
</LinearLayout>